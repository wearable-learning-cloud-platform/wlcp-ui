<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	xmlns:html="http://www.w3.org/1999/xhtml">
	
	<Dialog id="gameInstanceInfo" title = "Game Instance Info">
		<buttons>
			<Button text="Close" type="Accept" press="onCancelTilePress"/>
		</buttons>

		<content>
			<List items="{/list}" noData="No one is playing this game yet">
				<StandardListItem title="{key}"/>
			</List>
		</content>	
	</Dialog>
</core:FragmentDefinition>